# FireRunner Environment Configuration
# Copy this file to .env and fill in your values

# GitLab Configuration
GITLAB_URL=https://gitlab.com
GITLAB_TOKEN=your-gitlab-api-token-here
GITLAB_WEBHOOK_SECRET=your-webhook-secret-here

# Grafana Configuration
GRAFANA_PASSWORD=change-me-in-production

# Optional: Flintlock Configuration
FLINTLOCK_ENDPOINT=localhost:9090

# Optional: Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Optional: Server Configuration
SERVER_PORT=8080
METRICS_PORT=9090
